{
  "name": "get-feedback-now",
  "version": "1.0.0",
  "description": "An application for helping business owners get feedback on their product or service.",
  "main": "index.js",
  "author": "Brooks Benson",
  "license": "MIT",
  "engines": {
    "node": "8.8.1",
    "yarn": "1.2.1"
